{
  "name": "my-capabilities",
  "version": "0.0.0",
  "private": true,
  "description": "my-capabilities",
  "license": "OGL",
  "scripts": {
    "clean": "rimraf built",
    "compile": "tsc --outDir built",
    "build": "npm run clean && npm run compile && webpack --progress",
    "build:prod": "set NODE_ENV=production && npm run build",
    "dev": "concurrently -k \"npm run compile && nodemon built/app.server/server.js\" \"webpack-dev-server --inline --hot \"",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "dependencies": {
    "dotenv": "2.0.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "imports-loader": "0.6.5",
    "leaflet": "1.0.1",
    "moment": "2.15.2",
    "nprogress": "0.2.0",
    "query-string": "4.2.3",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "turf": "3.0.14",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@types/express": "4.0.33",
    "@types/leaflet": "1.0.36",
    "@types/nprogress": "0.0.29",
    "@types/query-string": "3.0.29",
    "@types/react": "0.14.44",
    "@types/react-dom": "0.14.18",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.0",
    "css-loader": "0.25.0",
    "csv-parser": "1.11.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.24.1",
    "node-sass": "3.10.1",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "0.9.5",
    "typescript": "2.0.6",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
